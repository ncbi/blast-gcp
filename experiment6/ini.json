{
    "source" :
    {
        "pubsub" :
        {
            "project_id" : "ncbi-sandbox-blast",
            "subscript_id" : "spark-test-subscript",
            "use" : "false"
        },
        "socket" :
        {
            "trigger_port" : 10012,
            "use" : "true"
        },
        "hdfs" :
        {
            "dir" : "hdfs:///user/%s/requests",
            "use" : "false"
        }
    },

    "blastjni" :
    {
        "db" :
        [
            {
                "selector" : "nt",
                "bucket"   : "nt_50mb_chunks",
                "pattern"  : "nt_50M",
                "extensions" : [ "nhr", "nin", "nsq" ],
                "num_partitions" : 160
            },
            {
                "selector" : "nr",
                "bucket"   : "nr_50mb_chunks",
                "pattern"  : "nr_50M",
                "extensions" : [ "phr", "pin", "psq" ],
                "num_partitions" : 160
            }
        ],
        "top_n" : 100
    },

    "result" :
    {
        "asn1" :
        {
            "bucket" : "blastgcp-pipeline-test",
            "gs_or_hdfs" : "time"
        },
        "status" :
        {
            "bucket"  : "blastgcp-pipeline-test"
        }
    },

    "spark" :
    {
        "transfer_files" : [ "libblastjni.so" ],
        "with_locality" : "true",
        "num_executors" : 8,
        "num_executor_cores" : 4,
        "shuffle_reduceLocality_enabled" : "false",
        "scheduler_fair" : "false",
        "parallel_jobs" : 2
    },

    "log" :
    {
        "port" : 10011,
        "pref_loc" : "false",
        "part_prep" : "true"
    }
}
