{
    "source" :
    {
        "pubsub" :
        {
            "project_id" : "ncbi-sandbox-blast",
            "subscript_id" : "spark-test-subscript",
            "use" : "true"
        },
        "max_backlog" : 10
    },

    "blastjni" :
    {
        "num_db_limit" : 15,
        "manifest_root" : "dbs_500M.json",
        "jni_log_level" : "INFO",
        "location" : "/tmp/blast/db",
        "num_locations" : 1,
        "lib_name" : "libblastjni.so"
    },

    "result" :
    {
        "asn1" :
        {
            "bucket" : "blastgcp-pipeline-test",
            "file" : "output/%s/seq-annot.asn",
            "gs_or_hdfs " : "gs"
        },
        "status" :
        {
            "bucket"  : "blastgcp-pipeline-test"
            "file" : "status/%s/status.txt",
            "running" : "RUNNING",
            "done" : "DONE",
            "error" : "ERROR"
        }
    },

    "spark" :
    {
        "transfer_files" : [ "libblastjni.so" ],
        "log_level" : "ERROR",
        "locality_wait" : "3s",
        "locality_mode" : 0,
        "with_dyn_alloc" : false,
        "executor_memory" : "",
        "shuffle_reduceLocality_enabled" : false,
        "scheduler_fair" : false,
        "parallel_jobs" : 6,
        "comm_port" : 10013,
        "num_executors" : 15,
        "num_executor_cores" : 2
    }

    "log" :
    {
        "host" : "blast-raetzw-m",
        "port" : 10011,
        "request" : true,
        "start" : false,
        "done" : false,
        "cutoff" : false,
        "final" : true,
        "part_prep" : false,
        "worker_shift" : false,
        "pref_loc" : false,
        "db_copy" : false
    }
}
