{
    "requests" :
    {
        "pubsub" :
        {
            "project_id" : "ncbi-sandbox-blast",
            "subscript_id" : "spark-test-subscript",
            "use" : "true"
        },
		"files" :
		{
			"dir" : "/panfs/somewhere",
			"use" : "false"
		},
		"socket" :
		{
			"port" : 12345,
			"use" : "false"
		},
		"max_backlog" : 12
    },

    "databases" :
    [
		{
			"key" : "nr",
			"worker_location" : "/tmp/blast/db",
			"source_location" : "gs://nr_50mb_chunks",
			"extensions" : [ "psq", "pin", "phr" ]
		},
		{
			"key" : "nt",
			"worker_location" : "/tmp/blast/db",
			"source_location" : "gs://nt_50mb_chunks",
			"extensions" : [ "nsq", "nin", "nhr" ]
		}
	],

    "results" :
    {
        "bucket" :
        {
	    	"bucket" : "blastgcp-pipeline-test/%s",
			"pattern" : "seq-annot.asn",
			"use" : true
        },
        "files" :
        {
            "dir"  : "/panfs/somewhere",
			"pattern" : "%s.asn",
			"use" : false
        }
    },

    "cluster" :
    {
		"transfer_files" : [ "libblastjni.so" ],
		"parallel_jobs" : 12,
        "num_executors" : 8,
        "num_executor_cores" : 2
    },

    "debug" :
    {
        "port" : 10011,
        "request" : true,
        "start" : true,
        "done" : true,
        "final" : true,
		"stkdrv_stats_log" : ""
    }

}
