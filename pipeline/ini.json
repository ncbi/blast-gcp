{
    "source" :
    {
        "pubsub" :
        {
            "project_id" : "ncbi-sandbox-blast",
            "subscript_id" : "integration-queue-dataproc-subscription-sprint7",
            "use" : "true"
        },
        "socket" :
        {
            "trigger_port" : 10012,
            "use" : "true"
        },
        "hdfs" :
        {
            "dir" : "hdfs:///user/%s/requests",
            "use" : "false"
        }
    },

    "blastjni" :
    {
        "db" :
        [
            {
                "selector" : "nt",
                "bucket"   : "nt_50mb_chunks",
                "pattern"  : "nt_50M",
                "extensions" : [ "nax", "nin", "nsq" ],
                "num_partitions" : 887,
                "num_locations" : 2
            },
            {
                "selector" : "nr",
                "bucket"   : "nr_50mb_chunks",
                "pattern"  : "nr_50M",
                "extensions" : [ "pax", "pin", "psq" ],
                "num_partitions" : 1086,
                "num_locations" : 1
            }
        ],
        "top_n" : 100,
        "num_locations" : 2,
        "manifest_root" : "dbs.json"
    },

    "result" :
    {
        "asn1" :
        {
            "bucket" : "blast-results",
            "file" : "%s/seq-annot.asn",
            "gs_or_hdfs" : "gs"
        },
        "status" :
        {
            "bucket"  : "blast-results",
            "file"    : "%s/status.txt"
        }
    },

    "spark" :
    {
        "transfer_files" : [ "libblastjni.so" ],
        "with_locality" : "true",
        "num_executors" : 182,
        "num_executor_cores" : 4,
        "shuffle_reduceLocality_enabled" : "false",
        "scheduler_fair" : "false",
        "parallel_jobs" : 1
    },

    "log" :
    {
        "port" : 10011,
        "pref_loc" : "false",
        "part_prep" : "true",
        "log_worker_shift": true,
        "log_job_start": true,
        "log_job_done": true,
        "log_cutoff": true
    }
}
